{
  "name": "react-boilerplate",
  "version": "1.0.1",
  "description": "Apptension's React Boilerplate",
  "repository": {
    "type": "git",
    "url": "git://github.com/react-boilerplate/react-boilerplate.git"
  },
  "engines": {
    "npm": ">=3",
    "node": ">=5"
  },
  "author": "Apptension",
  "license": "MIT",
  "scripts": {
    "server": "babel-watch ./app/server/server",
    "precommit": "lint-staged",
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "extract-intl": "babel-node --presets latest,stage-0 -- ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "postinstall": "npm run build:dll",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress",
    "build:clean": "npm run test:clean && rimraf ./dist/*",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "start": "cross-env NODE_ENV=development SHOW_VERSION=false node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "presetup": "npm i chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "postsetup": "npm run build:dll",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:watch": "esw app --color -w",
    "pretest": "npm run test:clean",
    "test": "cross-env NODE_ENV=test jest",
    "test:clean": "rimraf ./coverage",
    "test:watch": "npm run test -- --watch",
    "test:junit": "JEST_JUNIT_OUTPUT=./junit/junitOutput.xml npm test -- --ci --testResultsProcessor='jest-junit'",
    "test:coverage": "npm run test -- --coverage "
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist"
    ],
    "include": [
      "core-js",
      "eventsource-polyfill"
    ]
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "chalk": "2.4.1",
    "compression": "1.7.2",
    "cross-env": "5.1.6",
    "es5-shim": "^4.5.9",
    "eslint-config-apptension": "^1.1.5",
    "express": "^4.16.3",
    "fontfaceobserver": "2.0.13",
    "http": "^0.0.0",
    "http-status-codes": "^1.3.0",
    "ip": "1.1.5",
    "lodash": "^4.17.10",
    "lodash.defaultsdeep": "^4.6.0",
    "lodash.pullall": "^4.2.0",
    "lodash.uniq": "^4.5.0",
    "machina": "^2.0.2",
    "minimist": "1.2.0",
    "normalize.css": "^8.0.0",
    "p2": "^0.7.1",
    "pixi.js": "^4.8.1",
    "popmotion": "^8.3.3",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "semver-compare": "^1.0.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "ua-parser-js": "^0.7.18",
    "vector2d": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-jest": "^22.2.2",
    "babel-loader": "7.1.4",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-preset-latest": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chai": "^4.1.0",
    "chai-enzyme": "^1.0.0-beta.0",
    "chai-jest-diff": "^1.0.1",
    "cheerio": "1.0.0-rc.2",
    "circular-dependency-plugin": "4.4.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.1.0",
    "eslint": "4.19.1",
    "eslint-import-resolver-webpack": "0.10.0",
    "eslint-plugin-chai-friendly": "^0.4.0",
    "eslint-watch": "^3.1.5",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.7.0",
    "favicons-webpack-plugin": "^0.0.9",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "image-webpack-loader": "4.2.0",
    "imports-loader": "0.8.0",
    "inquirer-directory": "^2.1.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^22.3.0",
    "jest-enzyme": "^4.2.0",
    "jest-junit": "^3.1.0",
    "jest-styled-components": "^4.10.0",
    "json-server": "^0.12.2",
    "lint-staged": "6.1.1",
    "ngrok": "2.2.26",
    "nock": "^9.2.6",
    "node-plop": "0.9.0",
    "null-loader": "0.1.1",
    "offline-plugin": "4.9.0",
    "plop": "^1.9.1",
    "rimraf": "2.6.2",
    "shelljs": "0.8.2",
    "sinon": "^4.3.0",
    "sinon-chai": "^2.14.0",
    "style-loader": "^0.21.0",
    "url-loader": "0.6.2",
    "webpack": "3.11.0",
    "webpack-appversion-plugin": "^1.2.1",
    "webpack-dev-middleware": "2.0.5",
    "webpack-hot-middleware": "2.22.2",
    "webpack-spritesmith": "^0.4.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./internals/testing/test.index.js",
    "setupFiles": [
      "./internals/testing/setupRaf.js"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(spec))\\.jsx?$",
    "clearMocks": true,
    "collectCoverageFrom": [
      "app/**/*.js",
      "!app/main.js",
      "!app/i18n.js",
      "!**/node_modules/**"
    ],
    "moduleNameMapper": {
      "env-config": "<rootDir>/app/environment/test.js",
      "report-error": "<rootDir>/app/utils/reportError.js"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "\\.(jpg|jpeg|png|svg)$": "<rootDir>/internals/testing/fileTransformer.js"
    },
    "coverageDirectory": "coverage",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testURL": "http://localhost"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --ignore-path .gitignore --ignore-pattern internals/scripts -- --fix"
    ]
  }
}
